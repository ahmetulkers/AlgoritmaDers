#include <stdio.h>
int main()
{
    int N,terim1=0 , terim2=1 ,terim3;
   printf("Bir terim sayisi giriniz : ");
   scanf("%d",&N);
   
   if(N<=0)
   {
    printf("0'dan küçük olamaz başka bir sayi dene");
    scanf("%d",N);
   }
    printf("Fibonnaci Serisi : ");
   
    
   for(int i = 1; i<=N; ++i)
   {
    printf("%d ", terim1);
    terim3=terim1+terim2;
    terim1=terim2;
    terim2=terim3;


   }
    return 0;
   
}
